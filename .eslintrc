{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true,
        "jest": true
    },
    "extends": ["airbnb"],
    "rules": {
      "strict": "off",
      "import/prefer-default-export": "off",
      "no-underscore-dangle": "off",
      "no-console": "off"
    }, 
    "parserOptions": {
      "sourceType": "module"
    },
    "jsx-a11y/href-no-hash": "off",
    "jsx-a11y/anchor-is-valid": ["warn", { "aspects": ["invalidHref"] }]
}
